AddDefaultCharset UTF-8
DirectoryIndex Overview.html overview.html

RewriteEngine on
RewriteBase /csswg/

# Module Renames

RewriteRule ^css3-grid-align/(.*)$ css3-grid-layout/$2 [R=301,L]
RewriteRule ^css4-mediaqueries/(.*)$ mediaqueries4/$2 [R=301,L]


# Versioning System Switchover

RewriteRule ^css3-text-decor/(.*)$ css3-text-decor/$2 [R=301,L]


# Unversioned URLs

RewriteRule ^css-counter-styles/(.*)$ css-counter-styles-3/$2 [R=307,L]
RewriteRule ^css-text-decor/(.*)$ css-text-decor-3/$2 [R=307,L]
#RewriteRule ^css-xxx/(.*)$ css-xxx/$2 [R=307,L]
